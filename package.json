{
  "name": "<%= projectName %>",
  "version": "0.0.1",
  "private": true,
  "description": "<%= description %>",
  "templateInfo": {
    "name": "redux",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development RUN_ENV=development npm run build:weapp -- --watch",
    "dev:mock": "cross-env MOCK=true concurrently \"npm run dev\" \"npm run mock-server\"",
    "build:test": "npm run copy:ui && npm run lint && cross-env NODE_ENV=production RUN_ENV=development npm run build:weapp",
    "build": "npm run copy:ui && npm run lint && cross-env NODE_ENV=production RUN_ENV=production npm run build:weapp",
    "build:weapp": "npm run setappid && taro build --type weapp",
    "lint": "npm run lint:script",
    "fix": "npm run lint-fix:script",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint-fix:script": "npm run lint:script -- --fix",
    "mock-server": "nodemon scripts/mock-server.js",
    "cdn": "gulp --gulpfile scripts/gulpfile.js oss --prefix=dos/car-shopping-mall",
    "cdn:dev": "gulp --gulpfile scripts/gulpfile.js oss --prefix=dos/car-shopping-mall/dev",
    "clean:dist": "rimraf dist",
    "setappid": "gulp --gulpfile scripts/gulpfile.js setappid",
    "upload": "cross-env RUN_ENV=production node scripts/mp-ci.js",
    "upload:test": "cross-env RUN_ENV=development node scripts/mp-ci.js",
    "copy:ui": "gulp --gulpfile scripts/gulpfile.js copyWxappluiToSrcAssets"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@lx-frontend/util": "^1.1.0-beta.0",
    "@lx-frontend/wxapp-lui": "^1.3.31",
    "@tarojs/components": "3.3.9",
    "@tarojs/react": "3.3.9",
    "@tarojs/runtime": "3.3.9",
    "@tarojs/taro": "3.3.9",
    "ali-oss": "^6.16.0",
    "big.js": "^6.1.1",
    "dayjs": "^1.10.7",
    "decimal.js": "^10.3.1",
    "flyio": "^0.6.14",
    "lodash": "^4.17.21",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sentry-mina": "^0.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/cli": "^3.3.9",
    "@tarojs/mini-runner": "3.3.9",
    "@tarojs/webpack-runner": "3.3.9",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-preset-taro": "3.3.9",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-taro": "3.3.9",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "koa": "^2.13.4",
    "miniprogram-ci": "^1.6.1",
    "mockjs": "^1.1.0",
    "nodemon": "^2.0.13",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.1.0",
    "unzip": "^0.1.11"
  }
}
